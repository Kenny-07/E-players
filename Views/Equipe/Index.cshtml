@model Equipe
@{
    ViewData["Tittle"] = "Equipes";
}

<form method="POST" action='@Url.Action("Cadastrar")' >

    <label> ID </label>
    <input type= "text" name="IdEquipe" />

    <label> Nome </label>
    <input type="text" name="Nome" />

    <label> Imagem </label>
    <input type="text" name="imagem" />

    <button type="submit"> Cadastrar </button>

</form>

<table class="table table-striped table-responsive">

    <thead>

        <th> ID </th>
        <th> Nome </th>
        <th> Foto </th>

    </thead>
    <tbody>

        @foreach (Equipe equipe in ViewBag.Equipes)
        {
            <tr>
                <tr> @equipe.IdEquipe </tr>
                <tr> @equipe.Nome </tr>
                <tr> @equipe.Imagem </tr>
            </tr>
        }

    </tbody>

</table>